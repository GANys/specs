{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CityJSON metadata schema v1.1.0",
  
  "metadata": {
    "type": "object",
    "properties": {
      "citymodelIdentifier": {
        "type": "string"
      },
      "datasetTitle": {
        "type": "string"
      },
      "datasetReferenceDate": {
        "type": "string",
        "format": "date"
      },
      "geographicalExtent": {
        "type": "array",
        "items": {
          "type": "number"
        },
        "minItems": 6,
        "maxItems": 6
      },
      "geographicLocation": {
        "type": "string",
        "description": "the name of the area for instance"
      },
      "referenceSystem": {
        "type": "string",
        "pattern": "^(http|https)://www.opengis.net/def/crs/"
      }
    }
  }
}